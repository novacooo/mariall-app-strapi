{
    "kind": "collectionType",
    "collectionName": "employees",
    "info": {
        "singularName": "employee",
        "pluralName": "employees",
        "displayName": "Employee"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "firstName": {
            "type": "string",
            "required": true,
            "minLength": 1
        },
        "lastName": {
            "type": "string",
            "required": false,
            "minLength": 1
        },
        "quantities": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::quantity.quantity",
            "mappedBy": "employee"
        }
    }
}
